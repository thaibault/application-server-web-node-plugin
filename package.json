{
  "name": "serverwebnodeplugin",
  "version": "1.0.109",
  "license": "CC-BY-3.0",
  "description": "A generic extendable application and file server for webNode.",
  "homepage": "http://torben.website/serverWebNodePlugin",
  "documentationWebsite": {
    "name": "serverWebNodePlugin",
    "trackingCode": "UA-40192634-24"
  },
  "keywords": [
    "web",
    "backend",
    "application",
    "server"
  ],
  "main": "index.compiled.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/thaibault/serverWebNodePlugin.git"
  },
  "scripts": {
    "build": "webOptimizer build",
    "clear": "webOptimizer clear",
    "document": "webOptimizer document",
    "lint": "webOptimizer check:type; webOptimizer lint",
    "prepare": "npm run build",
    "report:coverage": "nyc report --reporter=text-lcov | coveralls",
    "serve": "npm run build && webNode",
    "start": "npm run serve",
    "test": "nyc npm run test:plain",
    "test:plain": "webOptimizer test",
    "update": "npm version patch && npm publish",
    "update:documentation": "documentationWebsite --log-level info",
    "watch": "webOptimizer build --watch"
  },
  "peerDependencies": {
    "babel-runtime": "*",
    "web-node": "*"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "clientnode": "^3.0.82",
    "coveralls": "^3.0.2",
    "documentation-website": "^1.0.135",
    "eslint": "^5.3.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-jsdoc": "^3.7.1",
    "flow-bin": "^0.79.0",
    "jsdoc": "^3.5.2",
    "nyc": "^12.0.2",
    "qunit": "^2.6.1",
    "web-node": "^1.0.194",
    "weboptimizer": "^2.0.185",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "webNode": {
    "server": {
      "application": {
        "rootPath": {
          "__evaluate__": "currentPath"
        },
        "options": {},
        "port": 8080,
        "hostName": "127.0.0.1"
      },
      "authentication": {
        "login": "admin",
        "password": "admin",
        "salt": "admin",
        "staticAssets": true
      },
      "hostNamePrefix": "",
      "hostNamePattern": ".+",
      "httpBasicAuthenticationCancelRedirectHTMLContent": "<html><head><meta charset=\"utf-8\" /><title>authenticationRedirect</title><meta http-equiv=\"refresh\" content=\"0; URL={1}\" /><script type=\"text/javascript\">window.location.href = '{1}';</script></head><body></body></html>",
      "redirects": {},
      "dynamicPathPrefix": "api",
      "staticResponses": {}
    }
  },
  "webOptimizer": {
    "files": {
      "compose": {
        "cascadingStyleSheet": null
      }
    },
    "module": {
      "aliases": {
        "#": "Use the pre-compiled artefacts.",
        "clientnode/test$": "clientnode/test.compiled",
        "web-node/configurator$": "web-node/configurator.compiled",
        "web-node/pluginAPI$": "web-node/pluginAPI.compiled"
      }
    },
    "targetTechnology": "node"
  }
}
